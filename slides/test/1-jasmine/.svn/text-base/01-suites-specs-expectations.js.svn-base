describe('Arithmetic operators', function () {
  it('should add numbers using + operator', function () {
    expect(1 + 2).toBe(3);
  });
  it('should subtract numbers using - operator', function () {
    expect(7 - 2).toBe(5);
  });
  it('should fail', function () {
    expect(4 * 2).toBe(42);
  });
});
